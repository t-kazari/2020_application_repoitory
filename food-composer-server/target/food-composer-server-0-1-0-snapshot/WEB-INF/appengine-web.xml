<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app
	xmlns="http://appengine.google.com/ns/1.0">

	<sessions-enabled>true</sessions-enabled>
	<runtime>java8</runtime>
	<threadsafe>true</threadsafe>
	<system-properties>
		<property name="java.util.logging.config.file"
			value="WEB-INF/prd/logging.properties" />
		<property name="spring.profiles.active" value="prd"></property>
	</system-properties>
	<vpc-access-connector>
		<name>projects/food-composer/locations/us-east1/connectors/gae-connector</name>
	</vpc-access-connector>

</appengine-web-app>
